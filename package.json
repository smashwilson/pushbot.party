  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{tsx,ts,d.ts} script/*",
    "lint": "prettier --check src/**/*.{tsx,ts,d.ts} script/*",
    "update-schema": "script/update-schema",
    "relay-compiler": "relay-compiler"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "relay": {
    "src": "./src",
    "schema": "./schema/pushbot.graphql",
    "language": "typescript",
    "artifactDirectory": "./src/__generated__"
  },
  "babelMacros": {
    "relay": {
      "artifactDirectory": "./src/__generated__"
    }
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@types/jest": "^29.0.1",
    "@types/memoize-one": "5.1.2",
    "@types/node": "^18.7.16",
