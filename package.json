{
  "name": "pushbot.party",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootswatch": "4.4.1",
    "chartist": "^0.11.4",
    "classnames": "2.2.6",
    "core-js": "3.6.4",
    "emoji-js": "^3.5.0",
    "graphql": "^14.6.0",
    "jquery": "^3.4.1",
    "memoize-one": "5.1.1",
    "moment": "^2.24.0",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-relay": "^9.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "react-twitter-embed": "3.0.3",
    "relay-runtime": "9.0.0",
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{tsx,ts,d.ts} script/*",
    "lint": "prettier --check src/**/*.{tsx,ts,d.ts} script/*",
    "update-schema": "script/update-schema",
    "relay-compiler": "relay-compiler --src ./src --schema ./schema/pushbot.graphql ---language typescript --artifactDirectory ./src/__generated__"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "relay": {
      "artifactDirectory": "./src/__generated__"
    }
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@types/chartist": "^0.9.47",
    "@types/classnames": "2.2.10",
    "@types/jest": "25.1.4",
    "@types/memoize-one": "5.1.2",
    "@types/node": "13.9.4",
    "@types/react": "16.9.26",
    "@types/react-copy-to-clipboard": "^4.3.0",
    "@types/react-dom": "16.9.5",
    "@types/react-relay": "7.0.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/relay-runtime": "8.0.7",
    "babel-plugin-relay": "9.0.0",
    "bootstrap": "^4.4.1",
    "prettier": "2.0.2",
    "relay-compiler": "^8.0.0",
    "relay-compiler-language-typescript": "10.1.5"
  }
}
