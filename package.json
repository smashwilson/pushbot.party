{
  "name": "pushbot.party",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootswatch": "4.6.0",
    "chartist": "^0.11.4",
    "classnames": "2.2.6",
    "core-js": "3.8.3",
    "emoji-js": "^3.5.0",
    "graphql": "^15.5.0",
    "jquery": "^3.5.1",
    "memoize-one": "5.1.1",
    "moment": "^2.29.1",
    "popper.js": "^1.16.1",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.1",
    "react-relay": "^10.1.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "react-twitter-embed": "3.0.3",
    "relay-runtime": "^10.1.3",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{tsx,ts,d.ts} script/*",
    "lint": "prettier --check src/**/*.{tsx,ts,d.ts} script/*",
    "update-schema": "script/update-schema",
    "relay-compiler": "relay-compiler --src ./src --schema ./schema/pushbot.graphql --language typescript --artifactDirectory ./src/__generated__"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "relay": {
      "artifactDirectory": "./src/__generated__"
    }
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "@types/chartist": "^0.11.0",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.20",
    "@types/memoize-one": "5.1.2",
    "@types/node": "14.14.28",
    "@types/react": "17.0.2",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "17.0.1",
    "@types/react-relay": "7.0.17",
    "@types/react-router-dom": "^5.1.7",
    "@types/relay-runtime": "^10.1.8",
    "babel-plugin-relay": "^10.1.3",
    "bootstrap": "^4.6.0",
    "prettier": "2.2.1",
    "relay-compiler": "^10.1.3",
    "relay-compiler-language-typescript": "13.0.4"
  }
}
