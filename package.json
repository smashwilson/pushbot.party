    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.6",
    "@types/react-relay": "^14.1.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/relay-runtime": "^14.1.2",
    "babel-plugin-relay": "^14.1.0",
    "bootstrap": "^5.2.1",
